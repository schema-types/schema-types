{
  "name": "schema-types",
  "version": "2.1.1",
  "description": "Runtime and compile-type type schema definition and validation",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/cjs/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "env TS_NODE_PROJECT=tsconfig.test.json ava",
    "test-coverage": "env TS_NODE_PROJECT=tsconfig.test.json nyc ava",
    "test-type-check": "tsc -p tsconfig.test.json",
    "type-check": "tsc"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/node": "^12",
    "@wessberg/rollup-plugin-ts": "^1.2.25",
    "ava": "^3.9.0",
    "ava-fast-check": "^1.1.2",
    "expect-type": "^0.7.4",
    "fast-check": "^1.25.1",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "rollup": "^2.16.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "funding": {
    "type": "GitHub",
    "url": "https://github.com/sponsors/jacobwgillespie"
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "keywords": [
    "typescript",
    "json",
    "schema",
    "json-schema",
    "types"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "reporter": [
      "text",
      "json"
    ]
  }
}
